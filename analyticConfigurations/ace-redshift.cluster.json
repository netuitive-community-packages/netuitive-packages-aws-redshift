{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "aws.redshift.cpuutilization",
        "properties": {
          "baseline": true,
          "correlation": true
        }
      },
      {
        "match": "aws.redshift.healthstatus",
        "properties": {
          "baseline": false,
          "correlation": false
        }
      },
      {
        "match": "aws.redshift.maintenancemode",
        "properties": {
          "baseline": false,
          "correlation": false
        }
      },
      {
        "match": "aws.redshift.network.*",
        "properties": {
          "baseline": true,
          "correlation": true
        }
      },
      {
        "match": "aws.redshift.read.*",
        "properties": {
          "baseline": true,
          "correlation": true
        }
      },
      {
        "match": "aws.redshift.write.*",
        "properties": {
          "baseline": true,
          "correlation": true
        }
      }
    ],
    "name": "Redshift Cluster",
    "scope": {
      "elementType": "Redshift Cluster"
    },
    "type": "ACE"
  }
}